{
  "name": "stearing_fox",
  "version": "1.0.14",
  "description": "Stearing Fox car dash application (Navigation, control panel, wheater, CAN)",
  "repository": {
    "type": "git",
    "url": "https://github.com/miroslawlis/steering-fox.git"
  },
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "debug": "node_modules\\.bin\\electron . debug",
    "prepare": "electron-build-env npm rebuild",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "electron-builder install-app-deps",
    "build-win": "electron-builder --x64 -w portable",
    "build-linux": "electron-builder  --armv7l --x64 -l appImage",
    "deploy": "electron-builder build --publish always --armv7l --x64 -l appImage",
    "release": "electron-builder --windows --linux --x64 --armv7l --publish always",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --publish onTag --x64 --armv7l -wl",
    "dist-win": "electron-builder --publish onTag --x64 -w portable",
    "dist-linux": "electron-builder --publish onTag --armv7l --x64 -l appImage",
    "dist-linux2": "electron-builder --dir --armv7l --x64 -l appImage"
  },
  "keywords": [
    "car",
    "dash",
    "js",
    "e83",
    "x3",
    "bmw",
    "carputer"
  ],
  "author": "Miros≈Çaw Lis, <aaa@aaa.aaa>",
  "license": "ISC",
  "devDependencies": {
    "electron": "^8.3.0",
    "electron-build-env": "0.2.0",
    "electron-reload": "1.4.0",
    "electron-builder": "^22.6.0"
  },
  "dependencies": {
    "bluetoothctl": "git+https://github.com/miroslawlis/bluetoothctl.git",
    "electron-updater": "^4.3.1",
    "ibus": "git+https://github.com/osvathrobi/node-ibus.git",
    "serialport": "^9.0.0"
  },
  "build": {
    "appId": "Stearing_FOX",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "icon": "build/icons/png/",
      "publish": "github"
    },
    "win": {
      "target": "portable",
      "icon": "build/icons/png/1024x1024.png",
      "publish": "github"
    }
  }
}